TableRepetition: WORepetition {
	list = allSheets;
	item = aSheet;
	index = sheetIndex;
}

HeaderRepetition: WORepetition {
	list = aSheet.headerRow;
	item = cell;
	index = headerIndex;
}

MappingRepetition: WORepetition {
	list = aSheet.mappingRow;
	item = cell;
	index = mappingIndex;
}

DataRowsRepetition: WORepetition {
	list = aSheet.dataRows;
	item = aRow;
	index = index;
}

ColRepetition: WORepetition {
    list  = aRow.dataCells;
    item  = cell;
    index = cellIndex;
}

ColHeading: WCFilteringSelect {
    list = "~@net.sf.webcat.outcomesmeasurement.UploadGradebookPage@COLUMNS";
    selection = "~columns[colIndex]";
    style = "width: 10.5em;";
}

TR: net.sf.webcat.core.TableRow {
    index = index;
}